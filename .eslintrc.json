{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": "off",
        "prettier/prettier": ["error"]
    }
}
